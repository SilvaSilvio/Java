package Polimorfismo;

public abstract class Funcionario {
	private String nome;
	private String clt;
	private Double Salario;
	
	
	
	

	public String getNome() {
		return nome;
	}

	public void setNome(String nome) {
		this.nome = nome;
	}

	public Double getSalario() {
		return Salario;
	}

	public void setSalario(Double salario) {
		Salario = salario;
	}

}
