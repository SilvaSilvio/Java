	double raio = 3;
	double area = pi * Math.pow(raio, raio);
	float a = 9;
	float x = (float) Math.sqrt(a);
	System.out.println(x);
	
	//System.out.println("informe qual a área");
	//raio = scan.nextDouble();
	System.out.printf("A Area do cirulo é %.4f%" , area);
	
	
	
