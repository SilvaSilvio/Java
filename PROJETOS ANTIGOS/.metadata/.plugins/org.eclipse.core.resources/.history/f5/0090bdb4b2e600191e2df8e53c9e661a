package EXERCICIOS;

import java.util.Scanner;

public class PlanoTelefone {

	public static void main(String[] args) {
Scanner scan = new Scanner(System.in);
	int plano;
	int minutos=0;
	float resultado;
	if(minutos <= 100) {
	plano = 50;	
	}else if(minutos > 100) {
		resultado = minutos + (minutos - 100) * 2; 
	}
	}

}
