package EstruturaRepeticao;

import java.util.Locale;
import java.util.Scanner;

public class For {

	public static void main(String[] args) {
		Locale.setDefault(Locale.US);
		Scanner scan = new Scanner(System.in);

		int x;
		for(x = 0; x < 10; x++) {
			x = scan.nextInt();
			x++;
			System.out.println(x);
		}
	
	}

}
